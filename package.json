{
  "name": "moviestime",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "pods": "cd ios && pod install && cd ..",
    "cleanAndroidBuild": "rm -rf node_modules && yarn && cd android && ./gradlew clean && cd .. && react-native run-android",
    "android": "cd android && ./gradlew clean && cd .. && react-native run-android",
    "release": "cd android && ./gradlew clean && ./gradlew assembleRelease && cd ..",
    "bundle": "cd android && ./gradlew clean && ./gradlew bundleRelease && cd .."
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.15.6",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-navigation/bottom-tabs": "^5.11.11",
    "@react-navigation/material-top-tabs": "^5.3.15",
    "@react-navigation/native": "^5.9.4",
    "@react-navigation/stack": "^5.14.5",
    "@reduxjs/toolkit": "^1.6.1",
    "axios": "^0.21.1",
    "identity-obj-proxy": "^3.0.0",
    "lottie-react-native": "^5.1.6",
    "prop-types": "^15.7.2",
    "react": "17.0.2",
    "react-native": "0.68.0",
    "react-native-config": "^1.4.2",
    "react-native-fast-image": "^8.6.3",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-image-viewing": "^0.2.0",
    "react-native-modalize": "^2.0.8",
    "react-native-reanimated": "^2.9.1",
    "react-native-safe-area-context": "^3.2.0",
    "react-native-screens": "^3.4.0",
    "react-native-splash-screen": "^3.3.0",
    "react-native-tab-view": "^2.16.0",
    "react-native-toast-message": "^1.4.9",
    "react-native-vector-icons": "^8.1.0",
    "react-redux": "^7.2.4"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.67.0",
    "react-test-renderer": "17.0.2"
  },
  "jest": {
    "preset": "react-native",
    "transformIgnorePatterns": [
      "node_modules/(?!@react-native|react-native)"
    ],
    "moduleNameMapper": {
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "identity-obj-proxy"
    }
  }
